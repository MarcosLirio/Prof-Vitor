algoritmo "semnome"
var
      num1, num2, soma : inteiro
inicio
      escreva("Digite o primeiro numero: ")
      leia(num1)
      
      escreva("Digite o segundo numero: ")
      leia(num2)
      
      soma:= (num1 + num2)
      escreva("O resultado de ", num1 , " + ", num2 ," é igual á " ,soma)
fimalgoritmo
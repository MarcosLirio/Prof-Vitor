algoritmo "semnome"
var
   saldoPositivo, cheque, total : real

inicio
    saldoPositivo:= 500,00
    cheque := 200,00
    total := (saldoPositivo + cheque)
    
    escreval("Seu saldo atual era de : ", saldoPositivo)
    escreval("Caiu um cheque de : ", cheque)
    escreva("Seu saldo atual agora é de : ", total)
fimalgoritmo